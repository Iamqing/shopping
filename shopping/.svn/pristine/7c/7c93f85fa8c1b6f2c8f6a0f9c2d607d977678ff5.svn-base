@charset "utf-8";
@import url("style.css");
@import url("footer.css");
/* 页眉布局样式 */
#header{ width:960px; height:100px;}
#header_line1{ width:960px; height:3px; background-color:#E0E0E0;}
#header_line2{ width:960px; height:3px; background-color:#F07700;}
#header_logo{ width:211px; height:77px;  float:left;}
#header_navigate{ width:160px; height:77px; float:right; text-align:right;}
#header_navigate li{ float:left; margin:5px 5px 0px 0px;}
.cartbuy{ width:14px; height:11px; background:url(bg1.gif) no-repeat 0px -238px;}

/*主体布局样式*/
#main{ width:960px; height:400px; border: 1px solid #ddd; background:url(reg_bg1.png) repeat-x; }
#main_title{line-height:38px; margin-left:20px; font-size:16px; font-weight:bold;}
.button_reg_out{ width:241px; height:36px; background:url(button_reg_out.png) no-repeat; border:none;}
.button_reg_over{width:241px; height:36px;background:url(button_reg_over.png) no-repeat; border:none;}
.input_text{ width:241px; height:30px; line-height:30px; border-width:1px; border-style:solid; padding:0px 5px;}
.input_text_normal{ border-color:#7F9DB9;}
.input_text_foucs{border-color:#FF8E42;}

